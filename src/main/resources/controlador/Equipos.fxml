<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.EquiposController">
   <children>
      <Button fx:id="btnAgregar" layoutX="40.0" layoutY="666.0" mnemonicParsing="false" onAction="#agregarPersona" prefHeight="25.0" prefWidth="149.0" text="Agregar Equipo" />
      <TableView fx:id="tblEquipos" layoutX="14.0" layoutY="63.0" prefHeight="469.0" prefWidth="1043.0">
        <columns>
            <TableColumn fx:id="colImagen" prefWidth="130.39999103546143" text="Imagen" />
          <TableColumn fx:id="colNombre" prefWidth="137.59999084472656" text="Nombre" />
          <TableColumn fx:id="colCampeonato" prefWidth="186.40003967285156" text="Campeonato" />
            <TableColumn fx:id="colWFV" minWidth="-1.0" prefWidth="110.40008544921875" text="WFV" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnModificar" layoutX="216.0" layoutY="666.0" mnemonicParsing="false" onAction="#modificar" prefHeight="25.0" prefWidth="169.0" text="Modificar Equipo" />
      <Button fx:id="btnEliminar" layoutX="412.0" layoutY="666.0" mnemonicParsing="false" onAction="#eliminar" prefHeight="25.0" prefWidth="156.0" text="Eliminar Equipo" />
      <Label layoutX="40.0" layoutY="29.0" text="Filtrar por nombre:" />
      <TextField fx:id="txtFiltrarNombre" layoutX="210.0" layoutY="24.0" onKeyReleased="#filtrarNombre" />
      <Button fx:id="btnAceptar" layoutX="888.0" layoutY="666.0" mnemonicParsing="false" onAction="#aceptar" prefHeight="25.0" prefWidth="156.0" text="Aceptar" />
   </children>
</AnchorPane>
